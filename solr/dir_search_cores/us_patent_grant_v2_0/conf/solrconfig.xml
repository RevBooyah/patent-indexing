    </lst>
  </requestHandler>


  <requestHandler name="advanced" class="solr.SearchHandler" >
	     <lst name="defaults">
	       <str name="echoParams">explicit</str>
	       <int name="rows">10</int>
		   <str name="facet.mincount">1</str>
	       <str name="facet.limit">10</str>
